          "extensions": {
            "position": 11
          },
          "_links": {
            "webui": "/display/fastpath/Event-Based+Plugins",
            "edit": "/pages/resumedraft.action?draftId=20745217&draftShareId=6491784c-3096-4f9e-8c8a-554e3189091b",
            "tinyui": "/x/AYw8AQ",
